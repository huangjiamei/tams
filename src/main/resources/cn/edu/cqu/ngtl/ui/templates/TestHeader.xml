    <bean id="TestHeader" parent="Uif-ApplicationHeaderBase" p:additionalCssClasses="navbar-fixed-top">
        <property name="applicationNavigation.brandImage">
            <bean parent="Uif-Image" p:style="width:150px;margin: 2px 5px;"
                  p:source="@{#ConfigProperties['application.url']}/images/logo.png" />
        </property>
        <property name="applicationNavigation.brandUrl" value="http://www.cqu.edu.cn" />

        <property name="applicationNavigation.navigationBarGroup.libraryCssClasses">
            <list merge="true">
                <!--在这里去掉collapse这个css类，避免右上角的内容被覆盖-->
                <!--<value>collapse</value>-->
                <value>navbar-collapse</value>
                <value>navbar-ex1-collapse</value>
            </list>
        </property>

        <property name="applicationNavigation.items">
            <list>
                <bean id="sessionTermName" parent="Uif-ActionLink"
                      p:actionLabel="@{new cn.edu.cqu.ngtl.dao.ut.impl.UTSessionDaoJpa().getCurrentSession().getTimeInfoName()}"
                      p:actionScript="showDialogTerm();"/>

                <!--导航栏-->
                <bean id="menuHeaderItem" parent="Uif-ActionLink" p:iconClass="icon-th" p:additionalCssClasses="mainMenu"
                      p:actionScript="showDialogMenu();"/>

                <!--user info-->
                <bean id="userInfo" parent="Uif-DropdownToggleMenu"
                      p:iconClass="icon-user" p:toggleText="@{user.getName()}">
                    <property name="menuItems">
                        <list>
                            <bean parent="Uif-VerticalBoxSection"
                                  p:additionalCssClasses="userInfo">
                                <property name="items">
                                    <list>
                                        <bean parent="Uif-Image"
                                              p:source="@{#ConfigProperties['application.url']}/images/user-header-default.jpg"
                                              p:additionalCssClasses="userInfoPic" />
                                        <bean parent="Uif-VerticalBoxSection"
                                              p:additionalCssClasses="userInfoMeg">
                                            <property name="items">
                                                <list>
                                                    <bean id="userDeptName" parent="Uif-MessageField" p:messageText="@{user.getDepartment()}"/>
                                                    <bean parent="Uif-MessageField" p:messageText="@{user.getTag()}"
                                                          p:style="font-size:16px" />
                                                </list>
                                            </property>
                                        </bean>

                                    </list>
                                </property>
                            </bean>
                            <bean parent="Uif-SecondaryActionButton-Small"
                                  p:actionLabel="注销" p:methodToCall="logout"
                                  p:additionalCssClasses="userInfoBtn" p:style="float:right">
                            </bean>

                        </list>
                    </property>

                </bean>


            </list>
        </property>
        <!--不知道这是什么但是一删就报错-->
        <property name="applicationToolbar">
            <bean id="toobar" parent="PortalToolbar" />
        </property>
    </bean>
    <!--不知道这是什么但是一删就报错-->
    <bean id="PortalToolbar" parent="Uif-CssGridGroup" p:render="@{!renderedInDialog}">
        <property name="template"
                  value="/krad/WEB-INF/ftl/components/group/groupContentWrap.ftl" />
        <property name="templateName" value="uif_groupContentWrap" />
        <property name="cssClasses">
            <list merge="true">
                <value>toolbar</value>
            </list>
        </property>
        <property name="items">
            <list>
                <bean parent="Uif-ListGroup"
                      p:layoutManager.additionalCssClasses="nav,pull-right">
                    <property name="items">
                        <list merge="true">
                        </list>
                    </property>
                </bean>
            </list>
        </property>
    </bean>


    <bean id="navDialog" parent="Uif-DialogGroup"
          p:dialogCssClass="navigationDialog"
          p:onShowDialogScript="initNavDialog();"
          p:style="overflow-y:hidden;">
        <property name="layoutManager">
            <bean parent="Uif-VerticalBoxLayout" />
        </property>
        <property name="items">
            <list>
                <bean parent="megamenu-items"/>
            </list>
        </property>

    </bean>

    <bean id="sessionTermFinderDialog" parent="Uif-DialogGroup">
        <property name="headerText" value="当前学期：@{new cn.edu.cqu.ngtl.dao.ut.impl.UTSessionDaoJpa().getCurrentSession().getTimeInfoName()}"/>
        <property name="dialogCssClass" value="confirmDialog"/>
        <property name="items">
            <list>
                <bean parent="Uif-VerticalBoxSection" >
                    <property name="items">
                        <list>
                            <bean parent="Uif-MessageField"
                                  p:messageText="请选择要替换的当前学期:"/>

                            <bean parent="Uif-InputField"
                                  p:propertyName="sessionTermFinder"
                                  p:style="margin: 5% 25% 0;"
                                  p:required="true"
                                  p:optionsFinderClass="cn.edu.cqu.ngtl.optionfinder.classview.sessionFinder">
                                <property
                                        name="control">
                                    <bean parent="Uif-DropdownControl"/>
                                </property>
                            </bean>

                        </list>
                    </property>
                </bean>
            </list>
        </property>
        <property name="footer.items">
            <list>
                <bean parent="Uif-DialogConfirmButton" p:actionLabel="确认"
                      p:methodToCall="xxxx"  p:dialogDismissOption="REQUEST"/>
                <bean parent="Uif-DialogDismissButton" p:actionLabel="取消"/>

            </list>
        </property>
    </bean>

