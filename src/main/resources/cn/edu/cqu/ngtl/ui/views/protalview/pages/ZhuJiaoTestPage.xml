<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xmlns:aop="http://www.springframework.org/schema/aop" xmlns:beans="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:jdbc="http://www.springframework.org/schema/jdbc" xmlns:jee="http://www.springframework.org/schema/jee"
       xmlns:jms="http://www.springframework.org/schema/jms" xmlns:lang="http://www.springframework.org/schema/lang"
       xmlns:mvc="http://www.springframework.org/schema/mvc" xmlns:oxm="http://www.springframework.org/schema/oxm"
       xmlns:task="http://www.springframework.org/schema/task" xmlns:tx="http://www.springframework.org/schema/tx"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/aop
                           http://www.springframework.org/schema/aop/spring-aop-3.0.xsd
                           http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context-3.0.xsd
                           http://www.springframework.org/schema/jdbc
                           http://www.springframework.org/schema/jdbc/spring-jdbc-3.0.xsd
                           http://www.springframework.org/schema/jee
                           http://www.springframework.org/schema/jee/spring-jee-3.0.xsd
                           http://www.springframework.org/schema/jms
                           http://www.springframework.org/schema/jms/spring-jms-3.0.xsd
                           http://www.springframework.org/schema/lang
                           http://www.springframework.org/schema/lang/spring-lang-3.0.xsd
                           http://www.springframework.org/schema/mvc
                           http://www.springframework.org/schema/mvc/spring-mvc-3.0.xsd
                           http://www.springframework.org/schema/oxm
                           http://www.springframework.org/schema/oxm/spring-oxm-3.0.xsd
                           http://www.springframework.org/schema/task
                           http://www.springframework.org/schema/task/spring-task-3.0.xsd
                           http://www.springframework.org/schema/tx
                           http://www.springframework.org/schema/tx/spring-tx-3.0.xsd
                           http://www.springframework.org/schema/util
                           http://www.springframework.org/schema/util/spring-util-3.0.xsd">

    <bean id="ZhuJiaoTestPage" parent="Uif-VerticalBoxSection"  p:additionalCssClasses="container" p:style="margin:0 auto;" >
        <property name="items">
            <list>

                <!--row 1-->
                <bean parent="Uif-VerticalBoxSection" p:additionalCssClasses="row">
                    <property name="items">
                        <list>
                            <bean parent="Uif-HorizontalBoxSection"
                                  p:additionalCssClasses="content-header col-md-10 col-md-offset-1">
                                <property name="items">
                                    <list>

                                        <!-- 左上角 -->
                                        <bean parent="Uif-HeaderThree" p:headerText="[b]助教考核[/b]" />


                                        <!-- 右上角 当前路径 -->
                                        <bean id="ZhuJiaoHeader" parent="Uif-HorizontalBoxSection"
                                              p:additionalCssClasses="breadcrumb">
                                            <property name="items">
                                                <list>
                                                    <bean parent="Uif-Icon" p:iconClass="icon-home myIcon"
                                                          p:style="float:left!important;"/>
                                                    <bean parent="Uif-InputField" p:style="clear:none!important;">
                                                        <property name="control">
                                                            <bean parent="Uif-OptionListControl"
                                                                  p:additionalCssClasses="breadcrumb"
                                                                  p:style="background: transparent!important;">
                                                                <property name="options">
                                                                    <list>
                                                                        <bean parent="Uif-KeyValueLocation" p:key="1" p:value="主页"
                                                                              p:location.viewId="PortalView" p:location.controllerMapping="/home"
                                                                              p:location.methodToCall="getCommonHome"/>
                                                                        <bean parent="Uif-KeyValueLocation" p:key="2" p:value="课程"
                                                                              p:location.viewId="PortalView" p:location.controllerMapping="/home"
                                                                              p:location.methodToCall="getCommonHome"/>
                                                                        <bean parent="Uif-KeyLabelPair" p:key="3" p:value="助教考核"/>
                                                                    </list>
                                                                </property>
                                                            </bean>
                                                        </property>
                                                    </bean>
                                                </list>
                                            </property>
                                        </bean>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>

                <bean parent="Uif-VerticalBoxSection" p:additionalCssClasses="box box-info">
                    <property name="items">
                        <list>
                            <bean parent="Uif-GridGroup"
                                  p:layoutManager.numberOfColumns="4"
                                  p:style="margin-top:10px">
                                <property name="headerText" value="考核信息"/>
                                <property name="items">
                                    <list>
                                        <bean parent="Uif-MessageField"
                                              p:label="课程"
                                              p:colSpan="3"
                                              p:messageText="高等数学"/>
                                        <bean parent="Uif-InputField"
                                              p:label="教师"
                                              p:defaultValue="阴文革"
                                              p:propertyName="inputField1"/>
                                        <bean parent="Uif-InputField"
                                              p:label="职工号"
                                              p:defaultValue="XXXXXXXXXX"
                                              p:propertyName="inputField2"/>
                                        <bean parent="Uif-InputField"
                                              p:label="课程编号"
                                              p:defaultValue="MATH12"
                                              p:propertyName="inputField3"/>
                                        <bean parent="Uif-InputField"
                                              p:label="课程类别"
                                              p:defaultValue="公共基础"
                                              p:propertyName="inputField4"/>
                                        <bean parent="Uif-InputField"
                                              p:label="授课专业"
                                              p:defaultValue="软件工程"
                                              p:propertyName="inputField5"/>
                                        <bean parent="Uif-InputField"
                                              p:label="授课年级"
                                              p:defaultValue="2016"
                                              p:propertyName="inputField6"/>
                                        <bean parent="Uif-InputField"
                                              p:label="必修/选修">
                                            <property name="control">
                                                <bean parent="Uif-DropdownControl">
                                                    <property name="options">
                                                        <list>
                                                            <bean parent="Uif-KeyValueLocation" p:key="1" p:value="必修"/>
                                                            <bean parent="Uif-KeyValueLocation" p:key="2" p:value="选修"/>
                                                        </list>
                                                    </property>
                                                </bean>
                                            </property>
                                        </bean>
                                        <bean parent="Uif-InputField"
                                              p:label="教学班号"
                                              p:defaultValue="101"
                                              p:propertyName="inputField7"/>
                                        <bean parent="Uif-InputField"
                                              p:label="学时"
                                              p:defaultValue="64"
                                              p:propertyName="inputField8"/>
                                        <bean parent="Uif-InputField"
                                              p:label="学分"
                                              p:defaultValue="5.0"
                                              p:propertyName="inputField9"/>
                                        <bean parent="Uif-InputField"
                                              p:label="学生人数"
                                              p:defaultValue="100"
                                              p:propertyName="inputField10"/>
                                        <bean parent="Uif-InputField"
                                              p:label="拟配备助教数"
                                              p:defaultValue="1"
                                              p:propertyName="inputField11"/>
                                    </list>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>

                <bean parent="Uif-Disclosure-TableCollectionSection" p:layoutManager.numberOfColumns="5">
                    <property name="headerText" value="考核任务"/>
                    <property name="collectionObjectClass" value="cn.edu.cqu.ngtl.dataobject.TestObject"/>
                    <!--<property name="collectionObjectClass" value="org.kuali.rice.krad.demo.uif.form.UITestObject"/>-->
                    <property name="renderAddBlankLineButton" value="true"/>
                    <property name="propertyName" value="collection"/>
                    <property name="layoutManager.generateAutoSequence" value="true"/>
                    <property name="layoutManager.richTable.render" value="true"/>
                    <property name="items">
                        <list>
                            <bean parent="Uif-InputField" p:label="任务类型" p:fieldLabel.wrapperCssClasses="demo-customHeader"
                                  p:propertyName="field1" p:required="true" p:wrapperCssClasses="demo-customCell1,demo-customCell2"/>
                            <bean parent="Uif-InputField" p:label="次数" p:propertyName="field2"
                                  p:required="true" p:wrapperCssClasses="demo-customCell3"/>
                            <bean parent="Uif-InputField" p:label="完成情况" p:propertyName="field3"
                                  p:required="true"/>
                            <bean parent="Uif-InputField" p:label="点赞比" p:propertyName="field4"
                                  p:required="true">
                            </bean>
                            <bean parent="Uif-InputField" p:label="评论" p:propertyName="field5"
                                  p:required="true"/>
                        </list>
                    </property>
                </bean>

                <bean parent="Uif-HorizontalBoxSection">
                    <property name="items">
                        <list>
                            <bean parent="Uif-HeaderTwo" p:headerText="[b]意见[/b]"/>
                            <bean parent="Uif-InputField" p:propertyName="inputField40">
                                <property name="control">
                                    <bean parent="Uif-TextAreaControl"
                                          p:style="width:75%"/>
                                </property>
                            </bean>
                            <bean parent="Uif-InputField"
                                  p:propertyName="inputField97">
                                <property name="control">
                                        <bean parent="Uif-DropdownControl">
                                            <property name="options">
                                                <list>
                                                    <bean parent="Uif-KeyValueLocation" p:key="1" p:value="优"/>
                                                    <bean parent="Uif-KeyValueLocation" p:key="2" p:value="良"/>
                                                    <bean parent="Uif-KeyValueLocation" p:key="2" p:value="中"/>
                                                    <bean parent="Uif-KeyValueLocation" p:key="2" p:value="差"/>
                                                </list>
                                            </property>
                                        </bean>
                                </property>
                            </bean>
                        </list>
                    </property>
                </bean>

                <bean parent="Uif-PrimaryActionButton" p:actionLabel="提交"
                      p:additionalCssClasses="col-md-6"
                      p:style="margin-top:35px:high:15px;width:40px;"
                      p:actionScript="alert('You clicked a button');"/>
            </list>
        </property>
    </bean>
</beans>