                                                                                  p:label="教学班号" p:messageText="001"/>
                                                                            <bean parent="Uif-MessageField"
                                                                                  p:label="上课教师人数" p:messageText="1"/>
                                                                            <bean parent="Uif-MessageField"
                                                                                  p:label="上课教师" p:messageText="阴文革"/>
                                                                            <bean parent="Uif-MessageField"
                                                                                  p:label="选课人数" p:messageText="400"/>
                                                                            <bean parent="Uif-MessageField"
                                                                                  p:label="拟配备的助教数" p:messageText="1"/>
                                                                            <bean parent="Uif-MessageField"
                                                                                  p:label="助教预计工时" p:messageText="40"/>
                                                                            <bean parent="Uif-MessageField"
                                                                                  p:label="成绩评定方式" p:messageText=" "/>
                                                                            <bean parent="Uif-MessageField"
                                                                                  p:label="平时成绩占考试成绩比例" p:messageText="40%"/>
                                                                        </list>
                                                                    </property>
                                                                </bean>
                                                            </list>
                                                        </property>
                                                    </bean>
                                                </list>
                                            </property>
                                        </bean>

                        </list>
