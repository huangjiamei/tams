<beans xmlns="http://www.springframework.org/schema/beans"
       xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:p="http://www.springframework.org/schema/p"
       xmlns:aop="http://www.springframework.org/schema/aop" xmlns:beans="http://www.springframework.org/schema/beans"
       xmlns:context="http://www.springframework.org/schema/context"
       xmlns:jdbc="http://www.springframework.org/schema/jdbc" xmlns:jee="http://www.springframework.org/schema/jee"
       xmlns:jms="http://www.springframework.org/schema/jms" xmlns:lang="http://www.springframework.org/schema/lang"
       xmlns:mvc="http://www.springframework.org/schema/mvc" xmlns:oxm="http://www.springframework.org/schema/oxm"
       xmlns:task="http://www.springframework.org/schema/task" xmlns:tx="http://www.springframework.org/schema/tx"
       xmlns:util="http://www.springframework.org/schema/util"
       xsi:schemaLocation="http://www.springframework.org/schema/aop
                           http://www.springframework.org/schema/aop/spring-aop-3.0.xsd
                           http://www.springframework.org/schema/beans
                           http://www.springframework.org/schema/beans/spring-beans-3.0.xsd
                           http://www.springframework.org/schema/context
                           http://www.springframework.org/schema/context/spring-context-3.0.xsd
                           http://www.springframework.org/schema/jdbc
                           http://www.springframework.org/schema/jdbc/spring-jdbc-3.0.xsd
                           http://www.springframework.org/schema/jee
                           http://www.springframework.org/schema/jee/spring-jee-3.0.xsd
                           http://www.springframework.org/schema/jms
                           http://www.springframework.org/schema/jms/spring-jms-3.0.xsd
                           http://www.springframework.org/schema/lang
                           http://www.springframework.org/schema/lang/spring-lang-3.0.xsd
                           http://www.springframework.org/schema/mvc
                           http://www.springframework.org/schema/mvc/spring-mvc-3.0.xsd
                           http://www.springframework.org/schema/oxm
                           http://www.springframework.org/schema/oxm/spring-oxm-3.0.xsd
                           http://www.springframework.org/schema/task
                           http://www.springframework.org/schema/task/spring-task-3.0.xsd
                           http://www.springframework.org/schema/tx
                           http://www.springframework.org/schema/tx/spring-tx-3.0.xsd
                           http://www.springframework.org/schema/util
                           http://www.springframework.org/schema/util/spring-util-3.0.xsd">

        <bean id="DeclareClassPage" parent="Uif-VerticalBoxSection"
              p:additionalCssClasses="container"
              p:style="margin:0 auto;">
            <property name="items">
                <list>
                    <!--row 1-->
                    <bean parent="Uif-VerticalBoxSection" p:additionalCssClasses="row">
                        <property name="items">
                            <list>
                                <bean parent="Uif-HorizontalBoxSection"
                                      p:additionalCssClasses="content-header col-md-10 col-md-offset-1">
                                    <property name="items">
                                        <list>

                                            <!-- 左上角 -->
                                            <bean parent="Uif-HeaderThree" p:headerText="[b]助教考核[/b]" />


                                            <!-- 右上角 当前路径 -->
                                            <bean id="DeclareClassHeader" parent="Uif-HorizontalBoxSection"
                                                  p:additionalCssClasses="breadcrumb">
                                                <property name="items">
                                                    <list>
                                                        <bean parent="Uif-Icon" p:iconClass="icon-home myIcon"
                                                              p:style="float:left!important;"/>
                                                        <bean parent="Uif-InputField" p:style="clear:none!important;">
                                                            <property name="control">
                                                                <bean parent="Uif-OptionListControl"
                                                                      p:additionalCssClasses="breadcrumb"
                                                                      p:style="background: transparent!important;">
                                                                    <property name="options">
                                                                        <list>
                                                                            <bean parent="Uif-KeyValueLocation" p:key="1" p:value="主页"
                                                                                  p:location.viewId="PortalView" p:location.controllerMapping="/home"
                                                                                  p:location.methodToCall="getCommonHome"/>
                                                                            <bean parent="Uif-KeyValueLocation" p:key="2" p:value="课程"
                                                                                  p:location.viewId="PortalView" p:location.controllerMapping="/home"
                                                                                  p:location.methodToCall="getCommonHome"/>
                                                                            <bean parent="Uif-KeyLabelPair" p:key="3" p:value="助教课程申请"/>
                                                                        </list>
                                                                    </property>
                                                                </bean>
                                                            </property>
                                                        </bean>
                                                    </list>
                                                </property>
                                            </bean>
                                        </list>
                                    </property>
                                </bean>
                            </list>
                        </property>
                    </bean>
                    <!--body-->
                    <bean parent="Uif-VerticalBoxSection" p:additionalCssClasses="box box-info">
                        <property name="items">
                                <list>
                                    <bean parent="Uif-TabGroup" p:title="cz1996">
                                        <property name="items">
                                            <list>
                                                <bean parent="Uif-VerticalBoxSection" p:headerText="课程信息">
                                                    <property name="items">
                                                        <list>
                                                            <bean parent="Uif-GridGroup"
                                                                  p:layoutManager.numberOfColumns="4"
                                                                  p:style="margin-top:10px">
                                                                <property name="items">
                                                                    <list>
                                                                        <bean parent="Uif-MessageField"
                                                                              p:label="课程"
                                                                              p:colSpan="3"
                                                                              p:messageText="@{applyViewObject.courseName}"/>
                                                                        <bean parent="Uif-MessageField"
                                                                              p:label="教师"
                                                                              p:messageText="阴文革"/>
                                                                              <!--p:propertyName="inputField1"/>-->
                                                                        <!--p:messageText="阴文革"-->
                                                                        <bean parent="Uif-MessageField"
                                                                              p:label="职工号"
                                                                              p:messageText="XXXXXXXXXX"/>
                                                                              <!--p:propertyName="inputField2"/>-->
                                                                        <!--p:messageText="XXXXXXXXXX"-->
                                                                        <bean parent="Uif-MessageField"
                                                                              p:label="课程编号"
                                                                              p:messageText="MATH12"/>
                                                                              <!--p:propertyName="inputField3"/>-->
                                                                        <!--p:messageText="MATH12"-->
                                                                        <bean parent="Uif-MessageField"
                                                                              p:label="课程类别"
                                                                              p:messageText="公共基础"/>
                                                                              <!--p:propertyName="inputField4"/>-->
                                                                        <!--p:messageText="公共基础"-->
                                                                        <bean parent="Uif-MessageField"
                                                                              p:label="授课专业"
                                                                              p:messageText="软件工程"/>
                                                                              <!--p:propertyName="inputField5"/>-->
                                                                        <!--p:messageText="软件工程"-->
                                                                        <bean parent="Uif-MessageField"
                                                                              p:label="授课年级"
                                                                              p:messageText="2016"/>
                                                                              <!--p:propertyName="inputField6"/>-->
                                                                        <!--p:messageText="2016"-->
                                                                        <bean parent="Uif-MessageField"
                                                                              p:label="必修/选修"
                                                                              p:messageText="必修"/>
                                                                              <!--p:propertyName="inputField55"/>-->
                                                                        <!--p:messageText="必修"-->
                                                                            <!--<property name="control">
                                                                                <bean parent="Uif-DropdownControl">
                                                                                    <property name="options">
                                                                                        <list>
                                                                                            <bean parent="Uif-KeyValueLocation" p:key="1" p:value="必修"/>
                                                                                            <bean parent="Uif-KeyValueLocation" p:key="2" p:value="选修"/>
                                                                                        </list>
                                                                                    </property>
                                                                                </bean>
                                                                            </property>-->
                                                                        <bean parent="Uif-MessageField"
                                                                              p:label="教学班号"
                                                                              p:messageText="101"/>
                                                                              <!--p:propertyName="inputField7"/>-->
                                                                        <!--p:messageText="101"-->
                                                                        <bean parent="Uif-MessageField"
                                                                              p:label="学时"
                                                                              p:messageText="64"/>
                                                                              <!--p:propertyName="inputField8"/>-->
                                                                        <!--p:messageText="64"-->
                                                                        <bean parent="Uif-MessageField"
                                                                              p:label="学分"
                                                                              p:messageText="5.0"/>
                                                                              <!--p:propertyName="inputField9"/>-->
                                                                        <!--p:messageText="5.0"-->
                                                                        <bean parent="Uif-MessageField"
                                                                              p:label="学生人数"
                                                                              p:messageText="100"/>
                                                                              <!--p:propertyName="inputField10"/>-->
                                                                        <!--p:messageText="100"-->
                                                                        <bean parent="Uif-MessageField"
                                                                              p:label="拟配备助教数"
                                                                              p:messageText="1"/>
                                                                              <!--p:propertyName="inputField11"/>-->
                                                                        <!--p:messageText="1"-->
                                                                    </list>
                                                                </property>
                                                            </bean>
                                                            <bean parent="Uif-Disclosure-TableCollectionSection" p:layoutManager.numberOfColumns="4">
                                                                <!--<property name="headerText" value="考核任务"/>-->
                                                                <property name="collectionObjectClass" value="cn.edu.cqu.ngtl.viewobject.classinfo.ClassTeacherViewObject"/>
                                                                <property name="renderAddBlankLineButton" value="true"/>
                                                                <property name="propertyName" value="classList"/>
                                                                <property name="layoutManager.generateAutoSequence" value="true"/>
                                                                <property name="layoutManager.richTable.render" value="true"/>
                                                                <property name="items">
                                                                    <list>
                                                                        <bean parent="Uif-InputField" p:label="任务类型" p:fieldLabel.wrapperCssClasses="demo-customHeader"
                                                                              p:propertyName="departmentName" p:required="true" p:wrapperCssClasses="demo-customCell1,demo-customCell2"/>
                                                                        <bean parent="Uif-InputField" p:label="次数" p:propertyName="departmentName"
                                                                              p:required="true" p:wrapperCssClasses="demo-customCell3"/>
                                                                        <bean parent="Uif-InputField" p:label="耗时" p:propertyName="departmentName"
                                                                              p:required="true"/>
                                                                        <bean parent="Uif-InputField" p:label="预算" p:propertyName="departmentName"
                                                                              p:required="true">
                                                                        </bean>
                                                                    </list>
                                                                </property>
                                                            </bean>

                                                            <bean parent="Uif-VerticalBoxSection" p:additionalCssClasses="box box-info">
                                                                <property name="items">
                                                                    <list>
                                                                        <bean parent="Uif-GridGroup"
                                                                              p:layoutManager.numberOfColumns="4">
                                                                            <property name="items">
                                                                                <list>
                                                                                    <bean parent="Uif-InputField"
                                                                                          p:label="耗时总计"
                                                                                          p:propertyName="eduBackground"
                                                                                          p:defaultValue="38"/>
                                                                                    <bean parent="Uif-InputField"
                                                                                          p:label="预算总计"
                                                                                          p:propertyName="eduBackground"
                                                                                          p:defaultValue="760"/>
                                                                                    </list>
                                                                             </property>
                                                                            </bean>
                                                                        </list>
                                                                </property>
                                                            </bean>

                                                            <bean parent="Uif-PrimaryActionButton" p:actionLabel="添加评定方式"
                                                                  p:actionScript="alert('You clicked a button');"/>
                                                            <bean parent="Uif-HeaderFive" p:headerText="上面那个按钮没用"/>
                                                            <bean parent="Uif-PrimaryActionButton" p:actionLabel="提交"
                                                                  p:actionScript="alert('You clicked a button');"/>
                                                        </list>
                                                    </property>
                                                </bean>
                                                <bean parent="Uif-VerticalBoxSection" p:headerText="反馈">
                                                    <property name="items">
                                                        <list>
                                                            <bean parent="Uif-VerticalBoxSection">
                                                                <property name="items">
                                                                    <list>
                                                                        <bean parent="Uif-HorizontalBoxSection" p:additionalCssClasses="callout callout-info" p:style="high:100px;width:100%;background:blue;">
                                                                            <property name="items">
                                                                                <list>
                                                                                    <bean parent="Uif-HeaderFour" p:headerText="任务时间过长，请秀给后重新提交"/>
                                                                                    <bean parent="Uif-HeaderFour" p:headerText="2016-08-20  10:25"
                                                                                          p:style="float:right"/>
                                                                                </list>
                                                                            </property>
                                                                        </bean>
                                                                        <bean parent="Uif-HorizontalBoxSection" p:additionalCssClasses="callout callout-info" p:style="high:100px;width:100%;background:red;margin-top:5px;">
                                                                            <property name="items">
                                                                                <list>
                                                                                    <bean parent="Uif-HeaderFour" p:headerText="审核通过,课程即将发布..."/>
                                                                                    <bean parent="Uif-HeaderFour" p:headerText="2016-08-20  15:30"
                                                                                          p:style="float:right"/>
                                                                                </list>
                                                                            </property>
                                                                        </bean>
                                                                    </list>
                                                                </property>
                                                            </bean>
                                                        </list>
                                                    </property>
                                                </bean>
                                            </list>
                                        </property>
                                    </bean>
                                </list>
                        </property>
                    </bean>
                </list>
            </property>
        </bean>
    </beans>